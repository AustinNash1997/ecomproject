<p id="notice"><%= notice %></p>

<h1>Categories</h1>
<br>
<div id="categories">
    <% @categories.each do |category| %>
        <%= render partial: 'categories/category',
        locals: {category: category} %>
    <% end %>

</div>
<br>
<div id='new'>
    <%= link_to 'New Category', new_category_path %>
</div>