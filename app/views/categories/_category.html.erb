<div id='category_partial'>
  <div id="category_content">

    <div id="category_image">
      <% if category.image.attached?%>
        <%= image_tag (category.image) %>
      <% end %>
    </div>

    <div id='category_name'>
      <p id='name'>
        <%= link_to category.name, category %>
      </p>
    </div>
  </div>
</div>